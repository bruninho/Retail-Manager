<li class="control-group nested-fields">
  <div class="controls">
    <%= f.label :basic_hours, "Basic Hours:" %>
    <%= f.text_field :basic_hours %>
    <%= f.label :sunday_bh_hours, "Sunday/BH Hours:" %>
    <%= f.text_field :sunday_bh_hours %>
  
    <%= f.fields_for :employee do |timesheet_payment_employee| %>
      <%= timesheet_payment_employee.text_field :id %>
    <% end %>
  
    <%= link_to_remove_association "remove", f %>
  </div>
</li>